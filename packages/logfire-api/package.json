{
  "name": "@pydantic/logfire-api",
  "private": false,
  "description": "JavaScript API for Logfire - https://pydantic.dev/logfire",
  "author": {
    "name": "The Pydantic Team",
    "email": "engineering@pydantic.dev",
    "url": "https://pydantic.dev"
  },
  "sideEffects": false,
  "homepage": "https://pydantic.dev/logfire",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "logfire",
    "observability",
    "opentelemetry",
    "tracing",
    "profiling",
    "stats",
    "monitoring"
  ],
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite build",
    "build": "vite build",
    "lint": "eslint",
    "preview": "vite preview",
    "typecheck": "tsc",
    "prepack": "cp ../../LICENSE .",
    "postpack": "rm LICENSE",
    "test": "vitest"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@pydantic/logfire-tooling-config": "*",
    "eslint": "^9.22.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}
