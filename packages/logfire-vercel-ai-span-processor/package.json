{
  "version": "0.1.0",
  "name": "@pydantic/logfire-vercel-ai-span-processor",
  "description": "Vercel AI Span Processor for Logfire - https://pydantic.dev/logfire",
  "author": {
    "name": "The Pydantic Team",
    "email": "engineering@pydantic.dev",
    "url": "https://pydantic.dev"
  },
  "sideEffects": false,
  "homepage": "https://pydantic.dev/logfire",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "logfire",
    "observability",
    "opentelemetry",
    "tracing",
    "ai",
    "vercel",
    "ai-sdk"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite build",
    "build": "vite build",
    "lint": "eslint --fix",
    "typecheck": "tsc",
    "prepack": "cp ../../LICENSE .",
    "postpack": "rm LICENSE",
    "test": "vitest --passWithNoTests"
  },
  "devDependencies": {
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "eslint": "^9.22.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "ai": "^4.3.15",
    "zod": "^3.23.8"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}
